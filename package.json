  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:client": "vite --host 0.0.0.0 --port 9001",
    "dev:serve": "NODE_ENV=development nodemon  server",
